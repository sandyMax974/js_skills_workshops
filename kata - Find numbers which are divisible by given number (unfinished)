//loops through array and % by divisor
//if result is 0 then push to result array

//WORKING CODE
function divisibleBy(numbers, divisor){
  let result = [];
  for (let i = 0; i < numbers.length; i++){
  if (numbers[i] % divisor == 0) {
    result.push(numbers[i])
    }
  } 
  return result
}

//USING .filter()
function divisibleBy(numbers, divisor){
  return result = numbers.filter (num => num % divisor == 0
  );
}
divisibleBy([1,2,3,4,5,6], 2)
