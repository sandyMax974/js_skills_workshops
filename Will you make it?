//SUDOCODE
if (fuelLeft * mpg) >= distanceToPump
then return true
else return false

//1ST DRAFT CODE
const zeroFuel = (distanceToPump, mpg, fuelLeft) => {
	if ((fuelLeft * mpg) >= distanceToPump) {
	return true
	}
	else {
	return false
	}
}

//REFINED CODE
const zeroFuel = (distanceToPump, mpg, fuelLeft) => {
  return (fuelLeft * mpg) >= distanceToPump ? true : false
}
